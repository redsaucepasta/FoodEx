<%- include("partials/header") -%>

<div class="centered">
  <h1>Welcome <%= user.name %></h1>
</div>
<br><br>



<div class="row">
  <% outletList.forEach(function(outlet){ %>
  <div class="col-sm-4">
    <div class="card">
      <div class="card-body">
        <h5 class="card-title card-centered"><%= outlet.name %></h5>
        <p class="card-text">With supporting text below as a natural lead-in to additional content.</p>
        <a class="btn btn-outline-primary btn-block" href="/menu/<%= outlet._id %>">Menu</a>
      </div>
    </div>
  </div>
  <% }); %>
</div>

<%- include("partials/footer") -%>
